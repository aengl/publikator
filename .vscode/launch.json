{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Organise",
      "args": ["${workspaceFolder}/src/cli.js", "organise", "test", "out"],
      "console": "integratedTerminal",
